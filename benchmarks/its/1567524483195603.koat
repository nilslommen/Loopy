(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS start))
(VAR x_8 x_5 x_2 x_6 x_3 x x_7 x_4 x_1)
(RULES
start(x_8,x_5,x_2,x_6,x_3,x,x_7,x_4,x_1) -> f(x_8,x_5,x_2,x_6,x_3,x,x_7,x_4,x_1)
f(x_8,x_5,x_2,x_6,x_3,x,x_7,x_4,x_1) -> f(-1+x_8,x_5,x_2,x_6,x_3,x,x_7,1+x_4,x_1) :|: 0<=x_4 && 0<=x_8 && -x_5+x_1=0 && -x_2+x_6=0 && 2<=x
)
