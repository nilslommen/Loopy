(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS start))
(VAR x_5 x_2 x_6 x_3 x x_7 x_4 x_1)
(RULES
start(x_5,x_2,x_6,x_3,x,x_7,x_4,x_1) -> f(x_5,x_2,x_6,x_3,x,x_7,x_4,x_1)
f(x_5,x_2,x_6,x_3,x,x_7,x_4,x_1) -> f(x_4,1+x_2,x_6,1+x_3,x,x_7,x_4,x_7) :|: x_2>-1 && x_6>x_3 && x_6>0 && x>1 && x_2<x && x_1>0 && x_7>3 && 2+x_5<=x_1
)
