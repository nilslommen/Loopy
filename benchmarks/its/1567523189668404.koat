(VAR x_5 x_2 x_6 x_3 x x_7 x_4 x_1 x_8)
(RULES
start(x_5,x_2,x_6,x_3,x,x_7,x_4,x_1,x_8) -> f(x_5,x_2,x_6,x_3,x,x_7,x_4,x_1,x_8)
