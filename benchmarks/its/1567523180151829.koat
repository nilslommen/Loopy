(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS start))
(VAR x_3 x x_7 x_4 x_1 x_5 x_2 x_6)
(RULES
start(x_3,x,x_7,x_4,x_1,x_5,x_2,x_6) -> f(x_3,x,x_7,x_4,x_1,x_5,x_2,x_6)
f(x_3,x,x_7,x_4,x_1,x_5,x_2,x_6) -> f(x_3,1+x,1+x_7,x_4,x_3,x_6,x_2,x_6) :|: x_7>-1 && x_2>x && x_2>0 && x_4>1 && x_7<x_4 && x_5>0 && x_6>3 && 2+x_1<=x_5
)
