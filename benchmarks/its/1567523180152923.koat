(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS start))
(VAR x_5 x_2 x_6 x_3 x x_7 x_4 x_1)
(RULES
start(x_5,x_2,x_6,x_3,x,x_7,x_4,x_1) -> f(x_5,x_2,x_6,x_3,x,x_7,x_4,x_1)
f(x_5,x_2,x_6,x_3,x,x_7,x_4,x_1) -> f(x_1,x_6,x_6,x_3,1+x,1+x_7,x_4,x_1) :|: x_7>-1 && x_3>x && x_3>0 && x_4>1 && x_7<x_4 && x_5>0 && x_1>2 && 2+x_2<=x_5
)
