(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS l0))
(VAR Arg_0 Arg_1 Arg_2 Arg_3 Arg_4 Arg_5)
(RULES
  l0(Arg_0,Arg_1,Arg_2,Arg_3,Arg_4,Arg_5) -> l1(Arg_0,Arg_1,Arg_2,Arg_3,Arg_4,Arg_5)
  l1(Arg_0,Arg_1,Arg_2,Arg_3,Arg_4,Arg_5) -> l1(Arg_0,Arg_1-1,Arg_2-1,Arg_3,1+Arg_4,Arg_5) :|: 1+Arg_3<=Arg_0+Arg_2 && 1+Arg_2<=Arg_3 && Arg_1+Arg_3<=Arg_0+Arg_2 && Arg_3<=Arg_0+Arg_2 && Arg_0+Arg_2<=Arg_1+Arg_3 && Arg_3+Arg_5<=Arg_2+Arg_4 && Arg_2+Arg_4<=Arg_3+Arg_5
)