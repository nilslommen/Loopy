(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS start))
(VAR x_4 x_1 x_5 x_2 x_6 x_3 x_7 x)
(RULES
start(x_4,x_1,x_5,x_2,x_6,x_3,x_7,x) -> f(x_4,x_1,x_5,x_2,x_6,x_3,x_7,x)
f(x_4,x_1,x_5,x_2,x_6,x_3,x_7,x) -> f(x_7,x,1+x_5,x_2,1+x_6,x_3,x_7,x) :|: x_5>-1 && x_2>x_6 && x_2>0 && x_3>1 && x_5<x_3 && x_4>0 && x_7>3 && 2+x_1<=x_4
)
