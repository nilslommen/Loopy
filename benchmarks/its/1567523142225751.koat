(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS start))
(VAR x_4 x_1 x_7 x_5 x_2 x_6 x_3 x)
(RULES
start(x_4,x_1,x_7,x_5,x_2,x_6,x_3,x) -> f(x_4,x_1,x_7,x_5,x_2,x_6,x_3,x)
f(x_4,x_1,x_7,x_5,x_2,x_6,x_3,x) -> f(x_4,x_1,x_5,x_5,1+x_2,1+x_6,x_3,x) :|: x_6<x_3 && x_6>0 && x>x_2 && x>-1 && x_5<=x_7 && x_7>0 && x_5>0
)
