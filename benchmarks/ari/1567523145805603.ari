(format LCTRS)
(theory Ints)
(fun f (-> Int Int Int Int Int Int))
(fun start (-> Int Int Int Int Int Int))
(entrypoint start)
(rule (start x_2 x_3 x x_4 x_1) (f x_2 x_3 x x_4 x_1))
(rule
(f x_2 x_3 x x_4 x_1)
(f 0 x_3 x (+ (- 1) x_4) x_1)
:guard
(and (and (> x_4 0) (= 0 x_2)) (> (+ (- 2) x) (+ (- 1) x_4))))
