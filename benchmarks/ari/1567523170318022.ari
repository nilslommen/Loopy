(format LCTRS)
(theory Ints)
(fun f (-> Int Int Int Int Int Int))
(fun start (-> Int Int Int Int Int Int))
(entrypoint start)
(rule (start x_4 x_1 x_2 x_3 x) (f x_4 x_1 x_2 x_3 x))
(rule
(f x_4 x_1 x_2 x_3 x)
(f x_4 x_1 (+ 1 x_2) x_3 x)
:guard
(and
(and (and (and (> x_3 0) (> x_3 x_2)) (> x_1 1)) (< x x_4))
(> x (- 1)))
)
