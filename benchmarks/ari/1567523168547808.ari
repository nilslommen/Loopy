(format LCTRS)
(theory Ints)
(fun f (-> Int Int Int Int Int Int Int))
(fun start (-> Int Int Int Int Int Int Int))
(entrypoint start)
(rule (start x x_3 x_4 x_1 x_5 x_2) (f x x_3 x_4 x_1 x_5 x_2))
(rule
(f x x_3 x_4 x_1 x_5 x_2)
(f x_5 x_3 x_4 x_1 x_5 (+ 1 x_2))
:guard
(and
(and (and (and (> x_4 (- 1)) (> x_4 x_2)) (<= x_5 x)) (> x 0))
(> x_5 0))
)
