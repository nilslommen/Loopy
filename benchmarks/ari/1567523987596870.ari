(format LCTRS)
(theory Ints)
(fun f (-> Int Int Int Int Int Int Int Int))
(fun start (-> Int Int Int Int Int Int Int Int))
(entrypoint start)
(rule (start x_3 x x_4 x_1 x_5 x_2 x_6) (f x_3 x x_4 x_1 x_5 x_2 x_6))
(rule
(f x_3 x x_4 x_1 x_5 x_2 x_6)
(f (+ 1 x_3) x_2 x_4 x_1 x_5 x_2 (+ (- 1) x_6))
:guard
(and (and (<= 0 (+ (- 1) x_6)) (<= 0 x_3)) (<= (+ 1 x) x_4)))
