(format LCTRS)
(theory Ints)
(fun f (-> Int Int Int Int Int Int))
(fun start (-> Int Int Int Int Int Int))
(entrypoint start)
(rule (start x x_4 x_1 x_2 x_3) (f x x_4 x_1 x_2 x_3))
(rule
(f x x_4 x_1 x_2 x_3)
(f (+ (- 1) x) x_2 x_1 x_2 x_1)
:guard
(and
(and (and (and (> x 0) (> x_3 0)) (<= (+ (- 2) x_2) x_4)) (> x_4 2))
(> x_2 3))
)
