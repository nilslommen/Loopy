(format LCTRS)
(theory Ints)
(fun f (-> Int Int Int Int Int Int Int))
(fun start (-> Int Int Int Int Int Int Int))
(entrypoint start)
(rule (start x_5 x_2 x x_3 x_1 x_4) (f x_5 x_2 x x_3 x_1 x_4))
(rule
(f x_5 x_2 x x_3 x_1 x_4)
(f x_5 x_2 x_5 x_3 (+ 1 x_1) x_4)
:guard
(and (<= 0 x_1) (<= (+ 1 x_1) x_4)))
