(format LCTRS)
(theory Ints)
(fun f (-> Int Int Int Int Int Int))
(fun start (-> Int Int Int Int Int Int))
(entrypoint start)
(rule (start x_1 x_2 x_3 x x_4) (f x_1 x_2 x_3 x x_4))
(rule
(f x_1 x_2 x_3 x x_4)
(f x_2 x_2 (+ (- 1) x_3) x x_4)
:guard
(and (and (and (> x_3 0) (> x_1 0)) (< x_1 x_4)) (> x_2 1)))
