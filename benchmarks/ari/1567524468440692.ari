(format LCTRS)
(theory Ints)
(fun f (-> Int Int Int Int Int Int Int Int Int Int))
(fun start (-> Int Int Int Int Int Int Int Int Int Int))
(entrypoint start)
(rule
(start x x_6 x_4 x_1 x_7 x_5 x_2 x_8 x_3)
(f x x_6 x_4 x_1 x_7 x_5 x_2 x_8 x_3))
(rule
(f x x_6 x_4 x_1 x_7 x_5 x_2 x_8 x_3)
(f x (+ 1 x_6) x_4 x_1 (+ (- 1) x_7) x_5 x_3 0 x_1)
:guard
(and (and (<= 0 x_7) (<= 0 x_6)) (= x_8 0)))
