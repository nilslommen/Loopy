(format LCTRS)
(theory Ints)
(fun f (-> Int Int Int Int Int Int))
(fun start (-> Int Int Int Int Int Int))
(entrypoint start)
(rule (start x_4 x_1 x_2 x_3 x) (f x_4 x_1 x_2 x_3 x))
(rule
(f x_4 x_1 x_2 x_3 x)
(f x_4 x_1 x_2 (+ (- 1) x_3) x_1)
:guard
(and (and (> x_3 0) (<= 1 (+ (- 1) x))) (<= 1 (+ (- 1) x_1))))
