(format LCTRS)
(theory Ints)
(fun f (-> Int Int Int Int Int Int Int))
(fun start (-> Int Int Int Int Int Int Int))
(entrypoint start)
(rule (start x x_3 x_1 x_4 x_2 x_5) (f x x_3 x_1 x_4 x_2 x_5))
(rule
(f x x_3 x_1 x_4 x_2 x_5)
(f x x_3 x_1 x x_2 (+ 1 x_5))
:guard
(and (<= 0 x_5) (<= (+ 1 x_5) x_3)))
