(format LCTRS)
(theory Ints)
(fun f (-> Int Int Int Int Int Int Int))
(fun start (-> Int Int Int Int Int Int Int))
(entrypoint start)
(rule (start x_5 x_2 x_3 x x_4 x_1) (f x_5 x_2 x_3 x x_4 x_1))
(rule
