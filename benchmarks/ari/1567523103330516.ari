(format LCTRS)
(theory Ints)
(fun f (-> Int Int Int Int Int Int))
(fun start (-> Int Int Int Int Int Int))
(entrypoint start)
(rule (start x_4 x_1 x_2 x_3 x) (f x_4 x_1 x_2 x_3 x))
(rule
(f x_4 x_1 x_2 x_3 x)
(f x_4 x_1 (+ (- 1) x_2) x_3 x_4)
:guard
(and (and (and (> x_2 0) (> x 0)) (< x x_3)) (> x_4 1)))
