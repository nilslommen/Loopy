(format LCTRS)
(theory Ints)
(fun f (-> Int Int Int Int Int))
(fun start (-> Int Int Int Int Int))
(entrypoint start)
(rule (start x_3 x x_1 x_2) (f x_3 x x_1 x_2))
(rule
(f x_3 x x_1 x_2)
(f (+ (- 1) x_3) (+ (- 1) x_3) x_1 (+ (- 1) x_3))
:guard
(and (and (and (> x_3 1) (> x_1 3)) (> x_2 0)) (= x_3 x)))
